_netssids(){
  local cur ssids
  cur="${COMP_WORDS[COMP_CWORD]}"

  if [ ${#COMP_WORDS[@]} == 2 ]; then
    ssids=`winfo --list 2>/dev/null; wscan -l --fuzzy 2>/dev/null`
    COMPREPLY=( $(compgen -W "$ssids" -- $cur) )
  else
    COMPREPLY=()
  fi
}

complete -F _netssids wconnect
complete -F _netssids winfo

